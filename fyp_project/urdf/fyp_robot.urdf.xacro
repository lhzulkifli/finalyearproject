<?xml version="1.0" ?>
<robot name="fyp_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:include filename="$(find fyp_project)/urdf/fyp_robot.gazebo.xacro" />
    
    <link name="base_footprint"/>

    <joint name="base_footprint__base_link" type="fixed">
        <parent link="base_footprint"/>
        <child link="base_link"/>
        <origin xyz="0 0 0.01" rpy ="0 0 0" />
    </joint>

    <link name="base_link">
      <inertial>
        <mass value="1" />
        <origin xyz="0 0 0.5" rpy="0 0 0" />
        <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
      </inertial>
      <collision>
        <origin xyz="0 0 0.5" rpy="0 0 0"/>
        <geometry>
          <box size="0.32 0.2 0.07" />
        </geometry>
      </collision>
      <visual>
        <origin xyz="0 0 0.5" rpy="0 0 0" />
        <geometry>
          <box size="0.32 0.2 0.07"/>
        </geometry>
      </visual>
    </link>

    <joint name="base_link__top_chasis" type="fixed">
        <parent link="base_link"/>
        <child link="top_chasis"/>
        <origin xyz="-0.03 0 0.56" rpy ="0 0 0" />
    </joint>

    <link name="top_chasis">
      <inertial>
        <mass value="1" />
        <origin xyz="0 0 0" rpy="0 0 0" />
        <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
      </inertial>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.21 0.18 0.05" />
        </geometry>
      </collision>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <box size="0.21 0.18 0.05"/>
        </geometry>
      </visual>
    </link>

    <joint name="base_link__backleft_wheel" type="fixed">
        <!--axis xyz="0 1 0"/-->
        <origin rpy="0 0 0" xyz="-0.1 0.109 0.465" />
        <parent link="base_link"/>
        <child link="backleft_wheel_link"/>
    </joint>

    <link name="backleft_wheel_link">
      <inertial>
        <mass value="1" />
        <origin rpy="0 0 0" xyz="0 0 0" />
        <inertia ixx="0.004317" ixy="0" ixz="0" iyy="0.004317" iyz="0" izz="0.0081" />
      </inertial>
      <collision>
        <origin rpy="1.5707 0 0" xyz="0 0 0" />
        <geometry>
          <cylinder length="0.018" radius="0.033" />
        </geometry>
      </collision>
      <visual>
        <origin rpy="1.5707 0 0" xyz="0 0 0" />
        <geometry>
          <cylinder length="0.018" radius="0.033" />
        </geometry>
      </visual>
    </link>

    <joint name="base_link__backright_wheel" type="fixed">
        <!--axis xyz="0 1 0"/-->
        <origin rpy="0 0 0" xyz="-0.1 -0.109 0.465" />
        <parent link="base_link"/>
        <child link="backright_wheel_link"/>
    </joint>

    <link name="backright_wheel_link">
      <inertial>
        <mass value="1" />
        <origin rpy="0 0 0" xyz="0 0 0" />
        <inertia ixx="0.004317" ixy="0" ixz="0" iyy="0.004317" iyz="0" izz="0.0081" />
      </inertial>
      <collision>
        <origin rpy="1.5707 0 0" xyz="0 0 0" />
        <geometry>
          <cylinder length="0.018" radius="0.033" />
        </geometry>
      </collision>
      <visual>
        <origin rpy="1.5707 0 0" xyz="0 0 0" />
        <geometry>
          <cylinder length="0.018" radius="0.033" />
        </geometry>
      </visual>
    </link>

    <joint name="base_link__frontleft_wheel" type="continuous">
        <axis xyz="0 1 0"/>
        <origin rpy="0 0 0" xyz="0.1 0.109 0.465" />
        <parent link="base_link"/>
        <child link="frontleft_wheel_link"/>
    </joint>

    <link name="frontleft_wheel_link">
      <inertial>
        <mass value="1" />
        <origin rpy="0 0 0" xyz="0 0 0" />
        <inertia ixx="0.004317" ixy="0" ixz="0" iyy="0.004317" iyz="0" izz="0.0081" />
      </inertial>
      <collision>
        <origin rpy="1.5707 0 0" xyz="0 0 0" />
        <geometry>
          <cylinder length="0.018" radius="0.033" />
        </geometry>
      </collision>
      <visual>
        <origin rpy="1.5707 0 0" xyz="0 0 0" />
        <geometry>
          <cylinder length="0.018" radius="0.033" />
        </geometry>
      </visual>
    </link>

    <joint name="base_link__frontright_wheel" type="continuous">
        <axis xyz="0 1 0"/>
        <origin rpy="0 0 0" xyz="0.1 -0.109 0.465" />
        <parent link="base_link"/>
        <child link="frontright_wheel_link"/>
    </joint>

    <link name="frontright_wheel_link">
      <inertial>
        <mass value="1" />
        <origin rpy="0 0 0" xyz="0 0 0" />
        <inertia ixx="0.004317" ixy="0" ixz="0" iyy="0.004317" iyz="0" izz="0.0081" />
      </inertial>
      <collision>
        <origin rpy="1.5707 0 0" xyz="0 0 0" />
        <geometry>
          <cylinder length="0.018" radius="0.033" />
        </geometry>
      </collision>
      <visual>
        <origin rpy="1.5707 0 0" xyz="0 0 0" />
        <geometry>
          <cylinder length="0.018" radius="0.033" />
        </geometry>
      </visual>
    </link>

    <joint name="base_link__imu_link" type="fixed">
      <parent link="base_link"/>
      <child link="imu_link"/>
      <origin xyz="0.1 0 0.5" rpy="0 0 0"/>
    </joint>

    <link name="imu_link">
     <visual>
       <origin xyz="0 0 0" rpy="0 0 0"/>
       <geometry>
         <box size="0.001 0.001 0.001"/>
       </geometry>
     </visual>
    </link>

    <joint name="top_chasis__base_scan" type="fixed">
      <parent link="top_chasis"/>
      <child link="base_scan"/>
      <origin xyz="0.03 0 0.045" rpy="0 0 0"/>
    </joint>

    <link name="base_scan">
      <inertial>
        <mass value="0.1" />
        <origin rpy="0 0 0" xyz="0 0 0" />
        <inertia ixx="0.004317" ixy="0" ixz="0" iyy="0.004317" iyz="0" izz="0.0081" />
      </inertial>
      <collision>
        <origin rpy="0 0 0" xyz="0 0 0" />
        <geometry>
          <cylinder length="0.0315" radius="0.055" />
        </geometry>
      </collision>
      <visual>
        <origin rpy="0 0 0" xyz="0 0 0" />
        <geometry>
          <cylinder length="0.0315" radius="0.055" />
        </geometry>
      </visual>
    </link>

  <joint name="base_link__camera_link" type="fixed">
    <origin xyz="0.17 0 0.53" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="camera_link"/>
  </joint>

  <link name="camera_link">
    <visual>
      <origin xyz="0.00 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.015 0.030 0.027"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.015 0.030 0.027"/>
      </geometry>
    </collision>
  </link>

  <joint name="camera_link__camera_rgb_frame" type="fixed">
    <origin xyz="-0.005 0 0.005" rpy="0 0 0"/>
    <parent link="camera_link"/>
    <child link="camera_rgb_frame"/>
  </joint>

  <link name="camera_rgb_frame"/>
    <!--visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.001 0.001 0.001"/>
      </geometry>
    </visual>
  </link-->

  <joint name="camera_rgb_frame__camera_rgb_optical_frame" type="fixed">
    <origin xyz="0 0 0" rpy="-1.57 0 -1.57"/>
    <parent link="camera_rgb_frame"/>
    <child link="camera_rgb_optical_frame"/>
  </joint>

  <link name="camera_rgb_optical_frame"/>
  
</robot>

    